        "@babel/preset-typescript": "^7.24.7",
        "@testing-library/jest-dom": "^6.4.6",
        "@types/jest": "^29.5.12",
        "@types/luxon": "^3.2.0",
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "babel-jest": "^29.7.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.14.47",
        "eslint": "^8.34.0",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^13.1.2",
        "obsidian": "^1.5.7-1",
