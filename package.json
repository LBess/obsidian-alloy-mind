    "license": "MIT",
    "devDependencies": {
        "@types/luxon": "^3.2.0",
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.14.47",
        "eslint": "^8.34.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.1.2",
        "obsidian": "^0.15.4",
        "prettier": "^2.8.4",
        "tslib": "2.4.0",
        "typescript": "4.7.4"
    },
    "lint-staged": {
        "*.ts": ["prettier --write", "eslint"]
    },
    "dependencies": {
        "luxon": "^3.2.1"
